RewriteEngine On

RewriteCond %{REQUEST_URI} \.(.{2,4})$
RewriteCond %{REQUEST_URI} !\.(php)$
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^(.*)$ $1 [L]

RewriteRule (.*)/(.*)/(.*)? index.php?_a_s_c=$1>$2>$3&%{QUERY_STRING} [L]
RewriteRule (.*)/(.*) index.php?_a_s_c=$1>>&_file=$2&%{QUERY_STRING} [L]


RewriteRule (.*) index.php?_a_s_c=webroot>>&_file=$1&%{QUERY_STRING} [L]





